# Uncomment the next line to define a global platform for your project

platform :ios, '9'

target 'BubbleRunner' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for BubbleRunner
  pod 'Google/SignIn'
  pod 'Firebase/Core'
  pod 'Firebase/AdMob'
  # pod 'Firebase'
  
  target 'BubbleRunnerTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BubbleRunnerUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
